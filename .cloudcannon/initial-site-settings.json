{
  "ssg": "astro",
  "build_configuration": {
    "preserved_paths": "node_modules/",
    "environment_variables": [
      { "key": "EDITING", "value": "true" },
      { "key": "PRODUCTION", "value": "true" }
    ],
    "install_command": "yarn install",
    "build_command": "yarn build",
    "output_path": "dist"
  }
}
