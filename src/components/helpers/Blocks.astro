---
import BlocksRenderer from './BlocksRenderer.svelte'

interface Props {
  pageData: {
    blocks: object[] | object
  }
  keys?: string[]
}

const { pageData, keys } = Astro.props
const liveRender = import.meta.env.EDITING === "true";
console.log('debuging en variables')
console.log(import.meta.env)
console.log(import.meta.env.EDITING)
console.log(import.meta.env.PRODUCTION)
console.log(process.env)
console.log(process.env.EDITING)
console.log(process.env.PRODUCTION)


---

{
  liveRender ? (
    <BlocksRenderer
      pageData={pageData}
      keys={keys}
      dataBinding={true}
      client:load
    />
  ) : (
    <BlocksRenderer
      pageData={pageData}
      keys={keys}
      dataBinding={false}
    />
  )
}
