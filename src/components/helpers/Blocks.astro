---
import BlocksRenderer from './BlocksRenderer.svelte'

interface Props {
  pageData: {
    blocks: object[] | object
  }
  keys?: string[]
}

const { pageData, keys } = Astro.props
const liveRender = false
---

{
  liveRender ? (
    <BlocksRenderer
      pageData={pageData}
      keys={keys}
      dataBinding={true}
      client:load
    />
  ) : (
    <BlocksRenderer
      pageData={pageData}
      keys={keys}
      dataBinding={false}
    />
  )
}
