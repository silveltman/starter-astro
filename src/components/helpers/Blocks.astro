---
import BlocksRenderer from './BlocksRenderer.svelte'

interface Props {
  pageData: {
    blocks: object[] | object
  }
  keys?: string[]
}

const { pageData, keys } = Astro.props
const liveRender = (import.meta.env.EDITING || "").trim() === "true";
console.log('debuging en variables')
console.log('LIVERENDER', liveRender)
console.log(import.meta.env.EDITING)
console.log('typeof import.meta.env.EDITING: ', typeof import.meta.env.EDITING);
---

{
  liveRender ? (
    <BlocksRenderer
      pageData={pageData}
      keys={keys}
      dataBinding={true}
      client:load
    />
  ) : (
    <BlocksRenderer
      pageData={pageData}
      keys={keys}
      dataBinding={false}
    />
  )
}
